{
  "name": "stopwarukraine",
  "description": "stopwarukraine",
  "authors": "ND",
  "version": "1.0.0",
  "main": "staticvue.js",
  "scripts": {
    "dev": "esno server",
    "start": "yarn build && yarn serve",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && yarn prerender",
    "serve": "cross-env NODE_ENV=production esno server.ts",
    "prerender": "esno prerender.ts",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "yarn lint:js && yarn lint:style",
    "commit": "git-cz",
    "deploy": "firebase deploy --only hosting"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.2.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/js-cookie": "^3.0.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.10",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "esno": "^0.14.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "serve-static": "^1.14.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-svg-loader": "^3.1.2"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.34",
    "@vuelidate/validators": "^2.0.0-alpha.26",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.24.0",
    "js-cookie": "^3.0.1",
    "jsdom": "^19.0.0",
    "sass": "^1.48.0",
    "vite-plugin-pages": "^0.20.0",
    "vue": "^3.2.25",
    "vue-i18n": "9",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.0.0",
    "vue3-lazyload": "^0.2.5-beta"
  }
}
